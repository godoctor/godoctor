package main

import "fmt"

type Pt struct {
	x, y int
}

func main() {
	a := 7
	b := 2
	p := Pt{3, 4}
	fmt.Println("Old Pt", p)
	p.x = 5
	c, n, p := foo(a, b, p)
	fmt.Println("Value of c is ", c)
	fmt.Println("Value of n is ", n)
	fmt.Println("Printing Modified point after returning", p)
}
func foo(a int, b int, p Pt) (int, int, Pt) {
	p.y = 6
	fmt.Println("New Pt", p)
	c := a * b
	n := p.x * p.y
	return c, n, p
}
